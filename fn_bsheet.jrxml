													LEFT JOIN adempiere.c_elementvalue elv ON el.second_level = elv.c_elementvalue_id
												WHERE fa.ad_client_id = $P{AD_Client_ID} AND ($P!{AD_Org_ID} IS NULL OR fa.ad_org_id = $P!{AD_Org_ID}) 
