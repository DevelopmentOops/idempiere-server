										UNION ALL
										
												SELECT second_level, elv.value, elv.name account_name, 0 amt_cur, SUM(amtacctdr-amtacctcr) amt_last,  0 amt_last1
												FROM adempiere.fact_acct fa
													LEFT JOIN adempiere.c_elementvalue_level_v el ON fa.account_id = el.first_level
													LEFT JOIN adempiere.c_elementvalue elv ON el.second_level = elv.c_elementvalue_id
												WHERE fa.ad_client_id = $P{AD_Client_ID} AND ($P!{AD_Org_ID} IS NULL OR fa.ad_org_id = $P!{AD_Org_ID}) 
												 	AND  fa.c_period_id = $P{C_Period_ID}-2 
