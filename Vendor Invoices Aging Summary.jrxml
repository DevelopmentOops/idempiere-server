	<queryString>
		<![CDATA[SELECT d.*, pt.name paymentterm

FROM

(SELECT	tot.c_bpartner_id, tot.customername, tot.bpgroup, sum(tot.days7) days7 ,sum(tot.days15) days15, sum(tot.days30) days30, sum(tot.days60) days60, sum(tot.days90) days90, sum(tot.days120) days120,
				SUM(tot.days45) days45,
				(SELECT rep.salesrep FROM (SELECT bp.name salesrep FROM adempiere.ad_user u LEFT JOIN adempiere.c_bpartner bp ON u.c_bpartner_id = bp.c_bpartner_id WHERE ad_user_id = $P{AD_User_ID})rep),
				(SELECT bp.bpname FROM (SELECT bp.name bpname FROM adempiere.c_bpartner bp  WHERE c_bpartner_id = $P{C_BPartner_ID})bp),
				(SELECT bpg.bpg_name FROM (SELECT bpg.name bpg_name FROM adempiere.c_bp_group bpg  WHERE c_bp_group_id = $P{C_BP_Group_ID})bpg),
				(SELECT org.orgname FROM (SELECT org.name orgname FROM adempiere.ad_org org  WHERE ad_org_id = $P{AD_Org_ID})org),
				(SELECT cl.clientname FROM (SELECT cl.name clientname FROM adempiere.ad_client cl  WHERE ad_client_id = $P{AD_Client_ID})cl)

FROM 

(SELECT	i.c_bpartner_id, customername, bpgroup,
	CASE WHEN ($P{DateAsOn} :: date - I.duedate :: date) BETWEEN 0 AND 7
	THEN openamt ELSE 0 END as days7,
	CASE WHEN ($P{DateAsOn} :: date - I.duedate :: date) BETWEEN 8 AND 15
	THEN openamt ELSE 0 END as days15,
	CASE WHEN ($P{DateAsOn} :: date - I.duedate :: date) BETWEEN 16 AND 30
	THEN openamt ELSE 0 END as days30,
	CASE WHEN ($P{DateAsOn} :: date - I.duedate :: date) BETWEEN 31 AND 45
	THEN openamt ELSE 0 END as days45,
	CASE WHEN ($P{DateAsOn} :: date - I.duedate :: date) BETWEEN 46 AND 60
	THEN openamt ELSE 0 END as days60,
	CASE WHEN ($P{DateAsOn} :: date - I.duedate :: date) BETWEEN 61 AND 90
	THEN openamt ELSE 0 END as days90,
	CASE WHEN ($P{DateAsOn} :: date - I.duedate :: date) BETWEEN 91 AND 100000000
	THEN openamt ELSE 0 END as days120

FROM

(
SELECT i.ad_org_id,
    i.ad_client_id,
    i.documentno,
    i.c_invoice_id,
    i.c_order_id,
    i.c_bpartner_id,
    i.issotrx,
    i.dateinvoiced,
    i.dateacct,
    p.netdays,
    adempiere.paymenttermduedate(i.c_paymentterm_id, i.dateinvoiced::timestamp with time zone) AS duedate,
    adempiere.paymenttermduedays(i.c_paymentterm_id, i.dateinvoiced::timestamp with time zone, adempiere.getdate()) AS daysdue,
    adempiere.adddays(i.dateinvoiced::timestamp with time zone, p.discountdays) AS discountdate,
    adempiere.currencyround(i.grandtotal * p.discount / 100::numeric, i.c_currency_id, 'N'::character varying) AS discountamt,
    i.grandtotal,
    adempiere.invoicepaid(i.c_invoice_id, i.c_currency_id, 1::numeric) AS paidamt,
    adempiere.invoiceopen(i.c_invoice_id, 0::numeric) AS openamt,
    i.c_currency_id,
    i.c_conversiontype_id,
    i.c_paymentterm_id,
    i.ispayschedulevalid,
    NULL::numeric AS c_invoicepayschedule_id,
    i.invoicecollectiontype,
    i.c_campaign_id,
    i.c_project_id,
    i.c_activity_id,
    i.c_invoice_ad_orgtrx_id AS ad_orgtrx_id,
    i.ad_user_id,
    i.c_bpartner_location_id,
    i.c_charge_id,
    i.c_doctype_id,
    i.c_doctypetarget_id,
    i.c_dunninglevel_id,
    i.chargeamt,
    i.c_payment_id,
    i.created,
    i.createdby,
    i.dateordered,
    i.dateprinted,
    i.description,
    i.docaction,
    i.docstatus,
    i.dunninggrace,
    i.generateto,
    i.isactive,
    i.isapproved,
    i.isdiscountprinted,
    i.isindispute,
    i.ispaid,
    i.isprinted,
    i.c_invoice_isselfservice AS isselfservice,
    i.istaxincluded,
    i.istransferred,
    i.m_pricelist_id,
    i.m_rma_id,
    i.paymentrule,
    i.poreference,
    i.posted,
    i.processedon,
    i.processing,
    i.ref_invoice_id,
    i.reversal_id,
    i.salesrep_id,
    i.sendemail,
    i.totallines,
    i.updated,
    i.updatedby,
    i.user1_id,
    i.user2_id,
    bp.name customername,
    bpg.c_bp_group_id,
    bpg.name bpgroup
   FROM adempiere.rv_c_invoice i
     JOIN adempiere.c_paymentterm p ON i.c_paymentterm_id = p.c_paymentterm_id
     JOIN adempiere.c_bpartner bp ON i.c_bpartner_id = bp.c_bpartner_id
     JOIN adempiere.c_bp_group bpg ON bp.c_bp_group_id = bpg.c_bp_group_id
  WHERE ($P!{C_BPartner_ID} IS NULL OR i.c_bpartner_id = $P!{C_BPartner_ID}) AND i.ad_client_id = $P{AD_Client_ID} 
	AND ($P!{C_BP_Group_ID} IS NULL OR bpg.c_bp_group_id = $P!{C_BP_Group_ID})
	AND adempiere.invoiceopen(i.c_invoice_id, 0::numeric) <> 0::numeric AND i.issotrx = $P{IsSoTrx}
	AND i.ispayschedulevalid <> 'Y'::bpchar AND (i.docstatus = ANY (ARRAY['CO'::bpchar, 'CL'::bpchar])) AND i.C_DocType_ID NOT IN (1000047, 1000048, 1000057) AND i.c_currency_id = 306
UNION
 SELECT i.ad_org_id,
    i.ad_client_id,
    i.documentno,
    i.c_invoice_id,
    i.c_order_id,
    i.c_bpartner_id,
    i.issotrx,
    i.dateinvoiced,
    i.dateacct,
    adempiere.daysbetween(ips.duedate::timestamp with time zone, i.dateinvoiced::timestamp with time zone) AS netdays,
    ips.duedate,
    adempiere.daysbetween(adempiere.getdate(), ips.duedate::timestamp with time zone) AS daysdue,
    ips.discountdate,
    ips.discountamt,
    ips.dueamt AS grandtotal,
    adempiere.invoicepaid(i.c_invoice_id, i.c_currency_id, 1::numeric) AS paidamt,
    adempiere.invoiceopen(i.c_invoice_id, ips.c_invoicepayschedule_id) AS openamt,
    i.c_currency_id,
    i.c_conversiontype_id,
    i.c_paymentterm_id,
    i.ispayschedulevalid,
    ips.c_invoicepayschedule_id,
    i.invoicecollectiontype,
    i.c_campaign_id,
    i.c_project_id,
    i.c_activity_id,
    i.c_invoice_ad_orgtrx_id AS ad_orgtrx_id,
    i.ad_user_id,
    i.c_bpartner_location_id,
    i.c_charge_id,
    i.c_doctype_id,
    i.c_doctypetarget_id,
    i.c_dunninglevel_id,
    i.chargeamt,
    i.c_payment_id,
    i.created,
    i.createdby,
    i.dateordered,
    i.dateprinted,
    i.description,
    i.docaction,
    i.docstatus,
    i.dunninggrace,
    i.generateto,
    i.isactive,
    i.isapproved,
    i.isdiscountprinted,
    i.isindispute,
    i.ispaid,
    i.isprinted,
    i.c_invoice_isselfservice AS isselfservice,
    i.istaxincluded,
    i.istransferred,
    i.m_pricelist_id,
    i.m_rma_id,
    i.paymentrule,
    i.poreference,
    i.posted,
    i.processedon,
    i.processing,
    i.ref_invoice_id,
    i.reversal_id,
    i.salesrep_id,
    i.sendemail,
    i.totallines,
    i.updated,
    i.updatedby,
    i.user1_id,
    i.user2_id,
    bp.name customername,
    bpg.c_bp_group_id,
    bp.name bpgroupg
   FROM adempiere.rv_c_invoice i
     JOIN adempiere.c_invoicepayschedule ips ON i.c_invoice_id = ips.c_invoice_id
     JOIN adempiere.c_bpartner bp ON i.c_bpartner_id = bp.c_bpartner_id
     JOIN adempiere.c_bp_group bpg ON bp.c_bp_group_id = bpg.c_bp_group_id
     JOIN adempiere.c_paymentterm p ON i.c_paymentterm_id = p.c_paymentterm_id
  WHERE adempiere.invoiceopen(i.c_invoice_id, ips.c_invoicepayschedule_id) <> 0::numeric AND i.ispayschedulevalid = 'Y'::bpchar
	AND (i.docstatus = ANY (ARRAY['CO'::bpchar, 'CL'::bpchar])) AND ips.isvalid = 'Y'::bpchar AND ($P!{C_BPartner_ID} IS NULL OR i.c_bpartner_id = $P!{C_BPartner_ID})
	AND i.ad_client_id = $P{AD_Client_ID}     AND i.C_DocType_ID NOT IN (1000047, 1000048, 1000057) AND i.c_currency_id = 306

ORDER BY duedate) I

WHERE	I.ad_client_id = $P{AD_Client_ID}
	AND I.duedate <= $P{DateAsOn}
	AND I.issotrx = $P{IsSoTrx}
	AND ($P!{AD_Org_ID} IS NULL OR I.ad_org_id = $P!{AD_Org_ID})
	AND ($P!{C_BPartner_ID} IS NULL OR I.c_bpartner_id = $P!{C_BPartner_ID})
	AND ($P!{C_BP_Group_ID} IS NULL OR I.c_bp_group_id = $P!{C_BP_Group_ID})
	AND ($P!{AD_User_ID} IS NULL OR I.salesrep_id = $P!{AD_User_ID})
	AND I.docstatus = 'CO'
	) tot
	
GROUP BY tot.bpgroup, tot.c_bpartner_id, tot.customername)d
	
LEFT JOIN adempiere.c_bpartner bp ON d.c_bpartner_id = bp.c_bpartner_id	
LEFT JOIN adempiere.c_paymentterm pt ON bp.PO_PaymentTerm_ID = pt.c_paymentterm_id
	
ORDER BY d.bpgroup, d.customername]]>
	</queryString>
