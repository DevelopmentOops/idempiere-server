	<parameter name="AD_Client_ID" class="java.lang.Integer"/>
	<parameter name="AD_Org_ID" class="java.lang.Integer"/>
	<parameter name="C_Period_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT d.*,gl_one.gl_amt gl_one,gl_two.gl_amt gl_two, gl_three.gl_amt gl_three,
(SELECT orgname FROM (SELECT org.name orgname FROM adempiere.ad_org org WHERE org.ad_org_id  = $P{AD_Org_ID} )org),
					(SELECT clientname FROM (SELECT cl.name clientname FROM adempiere.ad_client cl WHERE cl.ad_client_id = $P{AD_Client_ID} )cl),
					(SELECT curr_period FROM (SELECT p.name curr_period FROM adempiere.c_period p WHERE p.c_period_id = $P{C_Period_ID} )p),
					(SELECT last_period FROM (SELECT p.name last_period FROM adempiere.c_period p WHERE p.c_period_id = $P{C_Period_ID} -1 )p),
					(SELECT last_period1 FROM (SELECT p.name last_period1 FROM adempiere.c_period p WHERE p.c_period_id = $P{C_Period_ID} -2 )p)
					
FROM

(
					SELECT 0 grpid, second_level account_id, 1 order_by, value, account_name, SUM(amt_cur) amt_cur, SUM(amt_last) amt_last, SUM(amt_last1) amt_last1
					FROM 
									(
												SELECT second_level, elv.value, elv.name account_name, SUM(amtacctcr-amtacctdr) amt_cur, 0 amt_last, 0 amt_last1
												FROM adempiere.fact_acct fa
													LEFT JOIN adempiere.c_elementvalue_level_v el ON fa.account_id = el.first_level
													LEFT JOIN adempiere.c_elementvalue elv ON el.third_level = elv.c_elementvalue_id
												WHERE fa.ad_client_id = $P{AD_Client_ID} 
												 	AND ($P!{AD_Org_ID} IS NULL OR fa.ad_org_id = $P!{AD_Org_ID}) 
												    AND fa.c_period_id = $P{C_Period_ID}  
												    AND third_level IN (1000281, 10000498)
												GROUP BY second_level, elv.value, elv.name
												
										UNION ALL
										
												SELECT second_level, elv.value, elv.name account_name, 0 amt_cur, SUM(amtacctcr-amtacctdr) amt_last, 0 amt_last1
												FROM adempiere.fact_acct fa
													LEFT JOIN adempiere.c_elementvalue_level_v el ON fa.account_id = el.first_level
													LEFT JOIN adempiere.c_elementvalue elv ON el.third_level = elv.c_elementvalue_id
												WHERE fa.ad_client_id = $P{AD_Client_ID} 
												    AND ($P!{AD_Org_ID} IS NULL OR fa.ad_org_id = $P!{AD_Org_ID}) 
												 	AND  fa.c_period_id =  $P{C_Period_ID}-1 
												    AND third_level IN (1000281, 10000498)
												GROUP BY second_level, elv.value, elv.name
										
										UNION ALL
										
												SELECT second_level, elv.value, elv.name account_name, 0 amt_cur, 0 amt_last, SUM(amtacctcr-amtacctdr) amt_last1
												FROM adempiere.fact_acct fa
													LEFT JOIN adempiere.c_elementvalue_level_v el ON fa.account_id = el.first_level
													LEFT JOIN adempiere.c_elementvalue elv ON el.third_level = elv.c_elementvalue_id
												WHERE fa.ad_client_id = $P{AD_Client_ID} AND ($P!{AD_Org_ID} IS NULL OR fa.ad_org_id = $P!{AD_Org_ID}) 
												 	AND  fa.c_period_id =  $P{C_Period_ID}-2 
												    AND third_level IN (1000281, 10000498)
												GROUP BY second_level, elv.value, elv.name
												ORDER BY value) d
												GROUP BY second_level, value, account_name
						
					UNION ALL
					
					SELECT 1 grpid, second_level account_id, 2 order_by, value, account_name, SUM(amt_cur) amt_cur, SUM(amt_last) amt_last, SUM(amt_last1) amt_last1
						FROM 
										(
													SELECT second_level, elv.value, elv.name account_name, SUM(amtacctdr-amtacctcr) amt_cur, 0 amt_last, 0 amt_last1
													FROM adempiere.fact_acct fa
														LEFT JOIN adempiere.c_elementvalue_level_v el ON fa.account_id = el.first_level
														LEFT JOIN adempiere.c_elementvalue elv ON el.second_level = elv.c_elementvalue_id
													WHERE fa.ad_client_id = $P{AD_Client_ID} AND ($P!{AD_Org_ID} IS NULL OR fa.ad_org_id = $P!{AD_Org_ID}) 
														 AND  fa.c_period_id =  $P{C_Period_ID} 
														 AND fourth_level IN (1000288)
													GROUP BY second_level, elv.name, elv.value
													
										UNION ALL
													
													SELECT second_level, elv.value, elv.name account_name, 0 amt_cur, SUM(amtacctdr-amtacctcr) amt_last, 0 amt_last1
													FROM adempiere.fact_acct fa
														LEFT JOIN adempiere.c_elementvalue_level_v el ON fa.account_id = el.first_level
														LEFT JOIN adempiere.c_elementvalue elv ON el.second_level = elv.c_elementvalue_id
													WHERE fa.ad_client_id = $P{AD_Client_ID} AND ($P!{AD_Org_ID} IS NULL OR fa.ad_org_id = $P!{AD_Org_ID}) 
														 AND  fa.c_period_id = $P{C_Period_ID}-1 
														 AND fourth_level IN (1000288)
													GROUP BY second_level, elv.name, elv.value
										
										UNION ALL
										
													SELECT second_level, elv.value, elv.name account_name, 0 amt_cur, 0 amt_last, SUM(amtacctdr-amtacctcr) amt_last1
													FROM adempiere.fact_acct fa
														LEFT JOIN adempiere.c_elementvalue_level_v el ON fa.account_id = el.first_level
														LEFT JOIN adempiere.c_elementvalue elv ON el.second_level = elv.c_elementvalue_id
													WHERE fa.ad_client_id = $P{AD_Client_ID} AND ($P!{AD_Org_ID} IS NULL OR fa.ad_org_id = $P!{AD_Org_ID}) 
														 AND  fa.c_period_id = $P{C_Period_ID}-2 
														 AND fourth_level IN (1000288)
													GROUP BY second_level, elv.name, elv.value
													ORDER BY value) d
													GROUP BY second_level, value, account_name
						
					UNION ALL
					
					SELECT 2 grpid, second_level account_id, 3 order_by, value, account_name, SUM(amt_cur) amt_cur, SUM(amt_last) amt_last, SUM(amt_last1) amt_last1
					FROM 
										(
													SELECT second_level, elv.value, elv.name account_name, SUM(amtacctdr-amtacctcr) amt_cur, 0 amt_last, 0 amt_last1
													FROM adempiere.fact_acct fa
														LEFT JOIN adempiere.c_elementvalue_level_v el ON fa.account_id = el.first_level
														LEFT JOIN adempiere.c_elementvalue elv ON el.second_level = elv.c_elementvalue_id
													WHERE fa.ad_client_id = $P{AD_Client_ID} AND ($P!{AD_Org_ID} IS NULL OR fa.ad_org_id = $P!{AD_Org_ID}) 
														AND  fa.c_period_id = $P{C_Period_ID} 
													 	AND fourth_level IN (1000340)
													GROUP BY second_level, elv.name, elv.value
										
										UNION ALL
										
													SELECT second_level, elv.value, elv.name account_name, 0 amt_cur, SUM(amtacctdr-amtacctcr) amt_last, 0 amt_last1
													FROM adempiere.fact_acct fa
														LEFT JOIN adempiere.c_elementvalue_level_v el ON fa.account_id = el.first_level
														LEFT JOIN adempiere.c_elementvalue elv ON el.second_level = elv.c_elementvalue_id
													WHERE fa.ad_client_id = $P{AD_Client_ID} AND ($P!{AD_Org_ID} IS NULL OR fa.ad_org_id = $P!{AD_Org_ID}) 
													 	AND  fa.c_period_id =  $P{C_Period_ID}-1 
													 	AND fourth_level IN (1000340)
													GROUP BY second_level, elv.name, elv.value
										
										UNION ALL
										
													SELECT second_level, elv.value, elv.name account_name, 0 amt_cur, 0 amt_last, SUM(amtacctdr-amtacctcr)  amt_last1
													FROM adempiere.fact_acct fa
														LEFT JOIN adempiere.c_elementvalue_level_v el ON fa.account_id = el.first_level
														LEFT JOIN adempiere.c_elementvalue elv ON el.second_level = elv.c_elementvalue_id
													WHERE fa.ad_client_id = $P{AD_Client_ID} AND ($P!{AD_Org_ID} IS NULL OR fa.ad_org_id = $P!{AD_Org_ID}) 
													 	AND  fa.c_period_id =  $P{C_Period_ID}-2
													 	AND fourth_level IN (1000340)
													GROUP BY second_level, elv.name, elv.value
													ORDER BY value) d
													GROUP BY second_level, value, account_name
						
					UNION ALL
					
					SELECT 3 grpid, second_level account_id, 4 order_by, value, account_name, SUM(amt_cur) amt_cur, SUM(amt_last) amt_last, SUM(amt_last1) amt_last1
					FROM 
										(
												SELECT second_level, elv.value, elv.name account_name, SUM(amtacctdr-amtacctcr) amt_cur, 0 amt_last, 0 amt_last1
												FROM adempiere.fact_acct fa
													LEFT JOIN adempiere.c_elementvalue_level_v el ON fa.account_id = el.first_level
													LEFT JOIN adempiere.c_elementvalue elv ON el.second_level = elv.c_elementvalue_id
												WHERE fa.ad_client_id = $P{AD_Client_ID} AND ($P!{AD_Org_ID} IS NULL OR fa.ad_org_id = $P!{AD_Org_ID}) 
													AND  fa.c_period_id = $P{C_Period_ID} 
													AND second_level IN (1000098)
												GROUP BY second_level, elv.name, elv.value
												
										UNION ALL
										
												SELECT second_level, elv.value, elv.name account_name, 0 amt_cur, SUM(amtacctdr-amtacctcr) amt_last,  0 amt_last1
												FROM adempiere.fact_acct fa
													LEFT JOIN adempiere.c_elementvalue_level_v el ON fa.account_id = el.first_level
													LEFT JOIN adempiere.c_elementvalue elv ON el.second_level = elv.c_elementvalue_id
												WHERE fa.ad_client_id = $P{AD_Client_ID} AND ($P!{AD_Org_ID} IS NULL OR fa.ad_org_id = $P!{AD_Org_ID}) 
												 	AND  fa.c_period_id = $P{C_Period_ID}-2 
													AND second_level IN (1000098)
												GROUP BY second_level, elv.name, elv.value
												
										UNION ALL
										
												SELECT second_level, elv.value, elv.name account_name, 0 amt_cur, 0 amt_last,  SUM(amtacctdr-amtacctcr) amt_last1
												FROM adempiere.fact_acct fa
													LEFT JOIN adempiere.c_elementvalue_level_v el ON fa.account_id = el.first_level
													LEFT JOIN adempiere.c_elementvalue elv ON el.second_level = elv.c_elementvalue_id
												WHERE fa.ad_client_id = $P{AD_Client_ID} AND ($P!{AD_Org_ID} IS NULL OR fa.ad_org_id = $P!{AD_Org_ID}) 
												 	AND  fa.c_period_id = $P{C_Period_ID}-2 
												 	AND second_level IN (1000098)
												GROUP BY second_level, elv.name, elv.value
												ORDER BY value) d
												GROUP BY grpid, second_level, value, account_name
												ORDER BY grpid, order_by, value
) d
left join 
(select elv_o.second_level c_account_id,SUM(gl.amount) gl_amt
from adempiere.c_elementvalue_level_v elv_o
left join adempiere.gl_budget_idlogix gl ON elv_o.first_level = gl.c_elementvalue_id
where gl.c_period_id = $P{C_Period_ID}
group by elv_o.second_level) gl_one ON d.account_id = gl_one.c_account_id
left join
(select elv_o.second_level c_account_id,SUM(gl.amount) gl_amt
from adempiere.c_elementvalue_level_v elv_o
left join adempiere.gl_budget_idlogix gl ON elv_o.first_level = gl.c_elementvalue_id
where gl.c_period_id = $P{C_Period_ID}-1
group by elv_o.second_level) gl_two ON gl_one.c_account_id = gl_two.c_account_id
left join
(select elv_o.second_level c_account_id,SUM(gl.amount) gl_amt
from adempiere.c_elementvalue_level_v elv_o
left join adempiere.gl_budget_idlogix gl ON elv_o.first_level = gl.c_elementvalue_id
where gl.c_period_id = $P{C_Period_ID}-2
group by elv_o.second_level) gl_three ON gl_two.c_account_id = gl_three.c_account_id]]>
	</queryString>
