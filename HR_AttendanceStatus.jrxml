	<queryString>
		<![CDATA[SELECT emp.* 
, (SELECT COUNT(hr_final_attend_id) FROM adempiere.hr_final_attend WHERE c_bpartner_id = emp.c_bpartner_id
AND attdate::date BETWEEN $P{StartDate}::date AND (CASE WHEN now()::date <= $P{EndDate}::date THEN now()::date ELSE $P{EndDate}::date  END) AND attstatus = 'P') as Presents
, (SELECT array_to_string(array(SELECT EXTRACT(DAY FROM attdate::date) FROM adempiere.hr_final_attend 
WHERE c_bpartner_id = emp.c_bpartner_id AND attdate::date BETWEEN $P{StartDate}::date AND (CASE WHEN now()::date <= $P{EndDate}::date THEN now()::date ELSE $P{EndDate}::date  END)
AND (attstatus ='A' or attstatus = 'AB'        )),', ')) as Absents
, adempiere.getdtworkingdays_test(emp.c_bpartner_id::numeric, $P{StartDate}::timestamp without time zone,  (CASE WHEN now()::date <= $P{EndDate}::date THEN now()::date ELSE $P{EndDate}::date  END)::timestamp without time zone, 'WD'::character varying) as wdays
, (SELECT COUNT(atdate) FROM adempiere.xxattendance_v WHERE c_bpartner_id = emp.c_bpartner_id
AND atdate::date BETWEEN $P{StartDate}::date AND (CASE WHEN now()::date <= $P{EndDate}::date THEN now()::date ELSE $P{EndDate}::date  END) AND (latearrivalmins BETWEEN 1 AND 45)) as Lates
, (SELECT array_to_string(array(SELECT EXTRACT(DAY FROM attdate::date) FROM adempiere.hr_final_attend 
WHERE c_bpartner_id = emp.c_bpartner_id AND attdate::date BETWEEN $P{StartDate}::date AND  (CASE WHEN now()::date <= $P{EndDate}::date THEN now()::date ELSE $P{EndDate}::date  END) 
AND attstatus IN (SELECT value FROM adempiere.hr_levtypes WHERE value='AL' AND leaveunit = '1'  AND isaddsummary = 'Y'::bpchar)),', ')) as Leaves
, (SELECT array_to_string(array(SELECT EXTRACT(DAY FROM attdate::date) FROM adempiere.hr_final_attend 
WHERE c_bpartner_id = emp.c_bpartner_id AND attdate::date BETWEEN $P{StartDate}::date AND  (CASE WHEN now()::date <= $P{EndDate}::date THEN now()::date ELSE $P{EndDate}::date  END) 
AND attstatus IN (SELECT value FROM adempiere.hr_levtypes WHERE leaveunit = '0.33' AND isdeducted = 'Y'::bpchar  AND hr_levtypes_id NOT IN (1000024, 1000030, 1000031))),', ')) as ShortAbsents
, (SELECT array_to_string(array(SELECT EXTRACT(DAY FROM attdate::date) FROM adempiere.hr_final_attend 
WHERE c_bpartner_id = emp.c_bpartner_id AND attdate::date BETWEEN $P{StartDate}::date AND  (CASE WHEN now()::date <= $P{EndDate}::date THEN now()::date ELSE $P{EndDate}::date  END) 
AND attstatus IN (SELECT value FROM adempiere.hr_levtypes WHERE leaveunit = '0.66' AND isdeducted = 'Y'::bpchar  )),', ')) 
as ShortAbsents66
, (SELECT array_to_string(array(SELECT EXTRACT(DAY FROM attdate::date) FROM adempiere.hr_final_attend 
WHERE c_bpartner_id = emp.c_bpartner_id AND attdate::date BETWEEN $P{StartDate}::date AND  (CASE WHEN now()::date <= $P{EndDate}::date THEN now()::date ELSE $P{EndDate}::date  END) 
AND attstatus IN (SELECT value FROM adempiere.hr_levtypes WHERE leaveunit = '0.33' AND isdeducted = 'N'::bpchar  AND isaddsummary = 'Y'::bpchar AND hr_levtypes_id NOT IN (1000024, 1000030, 1000031))),', ')) as ShortLeave
, (SELECT array_to_string(array(SELECT EXTRACT(DAY FROM attdate::date) FROM adempiere.hr_final_attend 
WHERE c_bpartner_id = emp.c_bpartner_id AND attdate::date BETWEEN $P{StartDate}::date AND  (CASE WHEN now()::date <= $P{EndDate}::date THEN now()::date ELSE $P{EndDate}::date  END) 
AND attstatus IN (SELECT value FROM adempiere.hr_levtypes WHERE leaveunit = '0.5' AND isdeducted = 'Y'::bpchar)),', ')) as HalfAbsents
, (SELECT array_to_string(array(SELECT EXTRACT(DAY FROM attdate::date) FROM adempiere.hr_final_attend 
WHERE c_bpartner_id = emp.c_bpartner_id AND attdate::date BETWEEN $P{StartDate}::date AND  (CASE WHEN now()::date <= $P{EndDate}::date THEN now()::date ELSE $P{EndDate}::date  END) 
AND attstatus IN (SELECT value FROM adempiere.hr_levtypes WHERE leaveunit = '0.5' AND isdeducted = 'N'::bpchar AND isaddsummary = 'Y'::bpchar)),', ')) as HalfLeave
FROM adempiere.xxattendance_v emp
WHERE emp.ad_client_id =  $P{AD_Client_ID} 
	AND ($P!{AD_Org_ID} is null or emp.ad_org_id =$P!{AD_Org_ID})
		AND ($P!{C_BPartner_ID} is null or emp.c_bpartner_id =$P!{C_BPartner_ID})
				AND ($P!{HR_Department_ID} is null or emp.hr_department_id=$P!{HR_Department_ID})
AND ($P!{HR_EmpLocation_ID} is null or emp.hr_emplocation_id = $P!{HR_EmpLocation_ID}) 


    AND emp.atdate::date BETWEEN $P{StartDate}::date AND (CASE WHEN now()::date <= $P{EndDate}::date THEN now()::date ELSE $P{EndDate}::date  END)
    AND (CASE WHEN $P{Report_Type} = '1' THEN emp.enddate IS NULL  WHEN $P{Report_Type} = '2' THEN emp.enddate::date BETWEEN $P{StartDate}::date AND (CASE WHEN now()::date <= $P{EndDate}::date THEN now()::date ELSE $P{EndDate}::date  END) ELSE  (emp.enddate IS NULL OR emp.enddate::date >= $P{StartDate}::date) END)]]>
	</queryString>
