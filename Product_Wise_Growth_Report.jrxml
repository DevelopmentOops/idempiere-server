	<queryString>
		<![CDATA[SELECT d.prod_group2, d.product_name, SUM(COALESCE(d.cur_sales_amt, 0)) cur_sales_amt, SUM(COALESCE(d.last_sales_amt, 0)) last_sales_amt
				, SUM(lastqty) lastqty , SUM(curqty) curqty

			,(SELECT org.orgname FROM (SELECT org.name orgname FROM adempiere.ad_org org WHERE org.ad_org_id = $P{AD_Org_ID})org)
			,(SELECT cl.clientname FROM (SELECT cl.name clientname FROM adempiere.ad_client cl WHERE cl.ad_client_id = $P{AD_Client_ID} )cl)
			

FROM

(SELECT pg2.name prod_group2, pd.name product_name, SUM(amtacctcr-amtacctdr) cur_sales_amt, 0 last_sales_amt, ABS(SUM(fa.qty)) curqty, 0 lastqty

FROM adempiere.fact_acct fa
LEFT JOIN adempiere.m_product pd ON fa.m_product_id = pd.m_product_id
LEFT JOIN adempiere.M_Product_Group2 pg2 ON pd.M_Product_Group2_ID = pg2.M_Product_Group2_ID
LEFT JOIN adempiere.c_period pr ON fa.c_period_id = pr.c_period_id
LEFT JOIN adempiere.c_bpartner bp ON fa.c_bpartner_id = bp.c_bpartner_id
LEFT JOIN adempiere.c_bpartner_location bpl ON bp.c_bpartner_id = bpl.c_bpartner_id
LEFT JOIN adempiere.c_salesregion sr ON bpl.c_salesregion_id = sr.c_salesregion_id


WHERE	fa.ad_client_id =   $P{AD_Client_ID} AND fa.ad_org_id =  $P{AD_Org_ID} 
		AND fa.account_id = 1000128 AND fa.dateacct BETWEEN  $P{StartDate}  AND $P{EndDate} 
		AND ($P!{AD_User_ID} IS NULL OR sr.SalesRep_ID = $P!{AD_User_ID})
 		AND pr.periodtype !='A'

GROUP BY pg2.name, pd.name


UNION ALL

SELECT pg2.name prod_group2, pd.name product_name,  0 cur_sales_amt, SUM(amtacctcr-amtacctdr) last_sales_amt, 0 curqty, ABS(SUM(fa.qty)) lastqty

FROM adempiere.fact_acct fa
LEFT JOIN adempiere.c_bpartner bp ON fa.c_bpartner_id = bp.c_bpartner_id
LEFT JOIN adempiere.c_bpartner_location bpl ON bp.c_bpartner_id = bpl.c_bpartner_id
LEFT JOIN adempiere.c_salesregion sr ON bpl.c_salesregion_id = sr.c_salesregion_id
LEFT JOIN adempiere.m_product pd ON fa.m_product_id = pd.m_product_id
LEFT JOIN adempiere.M_Product_Group2 pg2 ON pd.M_Product_Group2_ID = pg2.M_Product_Group2_ID
LEFT JOIN adempiere.c_period pr ON fa.c_period_id = pr.c_period_id AND fa.dateacct between (select  $P{StartDate} :: date - INTERVAL '1 year') AND (select  $P{EndDate}:: date - INTERVAL '1 year')

WHERE	fa.ad_client_id =   $P{AD_Client_ID} AND fa.ad_org_id =  $P{AD_Org_ID}  
		AND ($P!{AD_User_ID} IS NULL OR sr.SalesRep_ID = $P!{AD_User_ID})
				AND fa.account_id = 1000128 AND pr.periodtype !='A'

GROUP BY pg2.name, pd.name ) d

GROUP BY d.prod_group2, d.product_name

ORDER BY d.prod_group2, d.product_name]]>
	</queryString>
