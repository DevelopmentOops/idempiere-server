	<queryString>
		<![CDATA[SELECT SUM(opening_cash) opening_cash, SUM(opening_bank) opening_bank, SUM(cash_receipts_dis) cash_receipts_dis,
		SUM(cash_receipts_oth) cash_receipts_oth, SUM(bank_receipts_dis) bank_receipts_dis, 
		SUM(bank_receipts_oth) bank_receipts_oth, SUM(vend_payments_cash) vend_payments_cash, SUM(other_payments_cash) other_payments_cash, SUM( vend_payments_bank)  vend_payments_bank,
		SUM(other_payments_bank) other_payments_bank,
		(SELECT (org.orgname) orgname FROM (SELECT org.name orgname FROM adempiere.ad_org org WHERE CAST(org.ad_org_id as integer) = CAST($P{AD_Org_ID} as integer))org),
		(SELECT (cl.clientname) clientname FROM (SELECT cl.name clientname FROM adempiere.ad_client cl WHERE CAST(cl.ad_client_id as integer) = CAST($P{AD_Client_ID} as integer))cl)
FROM

(SELECT sum(fa.amtacctdr-fa.amtacctcr) opening_cash, 0 opening_bank, 0 cash_receipts_dis, 0 cash_receipts_oth, 
		0 bank_receipts_dis, 0 bank_receipts_oth, 0 vend_payments_cash, 0 other_payments_cash, 0 vend_payments_bank,  0 other_payments_bank

FROM adempiere.fact_acct fa
LEFT JOIN adempiere.c_elementvalue elv ON fa.account_id = elv.c_elementvalue_id
LEFT JOIN adempiere.c_elementvalue_level_v elvl ON fa.account_id = elvl.first_level
LEFT JOIN adempiere.c_bankaccount ba ON elv.c_bankaccount_id = ba.c_bankaccount_id

WHERE fa.ad_client_id = $P{AD_Client_ID}  AND fa.ad_org_id =   $P{AD_Org_ID} 
		AND elvl.second_level = 1000514 
		AND fa.dateacct <   $P{StartDate} 
		
UNION ALL


SELECT 0 opening_cash, sum(fa.amtacctdr-fa.amtacctcr) opening_bank, 0 cash_receipts_dis, 0 cash_receipts_oth,
		0 bank_receipts_dis, 0 bank_receipts_oth, 0 vend_payments_cash, 0 other_payments_cash, 0 vend_payments_bank,  0 other_payments_bank

FROM adempiere.fact_acct fa
LEFT JOIN adempiere.c_elementvalue elv ON fa.account_id = elv.c_elementvalue_id
LEFT JOIN adempiere.c_elementvalue_level_v elvl ON fa.account_id = elvl.first_level

WHERE fa.ad_client_id =   $P{AD_Client_ID}  AND fa.ad_org_id =  $P{AD_Org_ID} 
		AND elvl.second_level IN (1000233, 1000259) AND fa.account_id != 1000523
		AND fa.dateacct <   $P{StartDate} 
		
UNION ALL

SELECT 0 opening_cash, 0 opening_bank, sum(fa.amtacctdr) cash_receipts_dis, 0 cash_receipts_oth,
		0 bank_receipts_dis, 0 bank_receipts_oth, 0 vend_payments_cash, 0 other_payments_cash, 0 vend_payments_bank,  0 other_payments_bank

FROM adempiere.fact_acct fa
LEFT JOIN adempiere.c_elementvalue elv ON fa.account_id = elv.c_elementvalue_id
LEFT JOIN adempiere.c_elementvalue_level_v elvl ON fa.account_id = elvl.first_level
LEFT JOIN adempiere.c_payment p ON fa.record_id = p.c_payment_id AND  fa.ad_table_id = 335
LEFT JOIN adempiere.gl_journal gj ON fa.record_id = gj.gl_journal_id AND fa.ad_table_id = 224
LEFT JOIN adempiere.c_bpartner bp ON fa.c_bpartner_id = bp.c_bpartner_id

WHERE fa.ad_client_id =   $P{AD_Client_ID}  AND fa.ad_org_id =   $P{AD_Org_ID} 
		AND elvl.second_level = 1000514 AND CONCAT(p.docstatus, gj.docstatus) IN ('CO', 'CL')
		AND fa.dateacct BETWEEN   $P{StartDate}  AND   $P{EndDate} AND bp.c_bp_group_id IN (1000001, 1000007)
		
UNION ALL

SELECT 0 opening_cash, 0 opening_bank, 0 cash_receipts_dis, sum(fa.amtacctdr) cash_receipts_oth,
		0 bank_receipts_dis, 0 bank_receipts_oth, 0 vend_payments_cash, 0 other_payments_cash, 0 vend_payments_bank,  0 other_payments_bank

FROM adempiere.fact_acct fa
LEFT JOIN adempiere.c_elementvalue elv ON fa.account_id = elv.c_elementvalue_id
LEFT JOIN adempiere.c_elementvalue_level_v elvl ON fa.account_id = elvl.first_level
LEFT JOIN adempiere.c_payment p ON fa.record_id = p.c_payment_id AND  fa.ad_table_id = 335 
LEFT JOIN adempiere.gl_journal gj ON fa.record_id = gj.gl_journal_id AND fa.ad_table_id = 224 
LEFT JOIN adempiere.c_bpartner bp ON fa.c_bpartner_id = bp.c_bpartner_id

WHERE fa.ad_client_id =  $P{AD_Client_ID}   AND fa.ad_org_id =   $P{AD_Org_ID} 
		AND elvl.second_level = 1000514 AND CONCAT(p.docstatus, gj.docstatus)  IN ('CO', 'CL')
		AND fa.dateacct BETWEEN  $P{StartDate}   AND $P{EndDate}   AND bp.c_bp_group_id NOT IN (1000001, 1000007)
		
UNION ALL

SELECT 0 opening_cash, 0 opening_bank, 0 cash_receipts_dis, 0 cash_receipts_oth,
		sum(fa.amtacctdr) bank_receipts_dis, 0 bank_receipts_oth, 0 vend_payments_cash, 0 other_payments_cash, 0 vend_payments_bank,  0 other_payments_bank

FROM adempiere.fact_acct fa
LEFT JOIN adempiere.c_elementvalue elv ON fa.account_id = elv.c_elementvalue_id
LEFT JOIN adempiere.c_elementvalue_level_v elvl ON fa.account_id = elvl.first_level
LEFT JOIN adempiere.c_bankstatement bs ON fa.record_id = bs.c_bankstatement_id AND fa.ad_table_id = 392
LEFT JOIN adempiere.c_bpartner bp ON fa.c_bpartner_id = bp.c_bpartner_id

WHERE fa.ad_client_id =  $P{AD_Client_ID}   AND fa.ad_org_id =   $P{AD_Org_ID}
		AND elvl.second_level IN (1000233, 1000259) AND bs.docstatus IN ('CO', 'CL')
		AND fa.dateacct BETWEEN   $P{StartDate}  AND  $P{EndDate}  AND bp.c_bp_group_id IN (1000001, 1000007)
		
UNION ALL

SELECT 0 opening_cash, 0 opening_bank, 0 cash_receipts_dis, 0 cash_receipts_oth,
		0 bank_receipts_dis, sum(fa.amtacctdr) bank_receipts_oth, 0 vend_payments_cash, 0 other_payments_cash, 0 vend_payments_bank,  0 other_payments_bank

FROM adempiere.fact_acct fa
LEFT JOIN adempiere.c_elementvalue elv ON fa.account_id = elv.c_elementvalue_id
LEFT JOIN adempiere.c_elementvalue_level_v elvl ON fa.account_id = elvl.first_level
LEFT JOIN adempiere.c_bankstatement bs ON fa.record_id = bs.c_bankstatement_id AND fa.ad_table_id = 392
LEFT JOIN adempiere.c_bpartner bp ON fa.c_bpartner_id = bp.c_bpartner_id

WHERE fa.ad_client_id =  $P{AD_Client_ID}   AND fa.ad_org_id =   $P{AD_Org_ID} 
		AND elvl.second_level IN (1000233, 1000259) AND bs.docstatus IN ('CO', 'CL')
		AND fa.dateacct BETWEEN   $P{StartDate} AND  $P{EndDate}  AND bp.c_bp_group_id NOT IN (1000001, 1000007)

UNION ALL

SELECT 0 opening_cash, 0 opening_bank, 0 cash_receipts_dis, 0 cash_receipts_oth,
		0 bank_receipts_dis, 0 bank_receipts_oth, sum(fa.amtacctcr) vend_payments_cash, 0 other_payments_cash, 0 vend_payments_bank,  0 other_payments_bank

FROM adempiere.fact_acct fa
LEFT JOIN adempiere.c_elementvalue elv ON fa.account_id = elv.c_elementvalue_id
LEFT JOIN adempiere.c_elementvalue_level_v elvl ON fa.account_id = elvl.first_level
LEFT JOIN adempiere.c_payment p ON fa.record_id = p.c_payment_id AND  fa.ad_table_id = 335 
LEFT JOIN adempiere.gl_journal gj ON fa.record_id = gj.gl_journal_id AND fa.ad_table_id = 224 
LEFT JOIN adempiere.c_bpartner bp ON fa.c_bpartner_id = bp.c_bpartner_id

WHERE fa.ad_client_id =    $P{AD_Client_ID} AND fa.ad_org_id =   $P{AD_Org_ID} 
		AND elvl.second_level = 1000514 AND CONCAT(p.docstatus, gj.docstatus)  IN ('CO', 'CL')
		AND fa.dateacct BETWEEN   $P{StartDate}  AND $P{EndDate}   AND bp.c_bp_group_id IN (1000004, 1000005, 1000006)
		
UNION ALL

SELECT 0 opening_cash, 0 opening_bank, 0 cash_receipts_dis, 0 cash_receipts_oth,
		0 bank_receipts_dis, 0 bank_receipts_oth,0 vend_payments_cash,  sum(fa.amtacctcr) other_payments_cash, 0 vend_payments_bank,  0 other_payments_bank

FROM adempiere.fact_acct fa
LEFT JOIN adempiere.c_elementvalue elv ON fa.account_id = elv.c_elementvalue_id
LEFT JOIN adempiere.c_elementvalue_level_v elvl ON fa.account_id = elvl.first_level
LEFT JOIN adempiere.c_payment p ON fa.record_id = p.c_payment_id AND  fa.ad_table_id = 335 
LEFT JOIN adempiere.gl_journal gj ON fa.record_id = gj.gl_journal_id AND fa.ad_table_id = 224 
LEFT JOIN adempiere.c_bpartner bp ON fa.c_bpartner_id = bp.c_bpartner_id

WHERE fa.ad_client_id =    $P{AD_Client_ID} AND fa.ad_org_id =   $P{AD_Org_ID} 
		AND elvl.second_level = 1000514 AND CONCAT(p.docstatus, gj.docstatus)   IN ('CO', 'CL')
		AND fa.dateacct BETWEEN   $P{StartDate}  AND $P{EndDate}   AND bp.c_bp_group_id NOT IN (1000004, 1000005, 1000006)


UNION ALL

SELECT 0 opening_cash, 0 opening_bank, 0 cash_receipts_dis, 0 cash_receipts_oth,
		0 bank_receipts_dis, 0 bank_receipts_oth, 0 vend_payments_cash, 0 other_payments_cash, sum(fa.amtacctcr) vend_payments_bank,  0 other_payments_bank

FROM adempiere.fact_acct fa
LEFT JOIN adempiere.c_elementvalue elv ON fa.account_id = elv.c_elementvalue_id
LEFT JOIN adempiere.c_elementvalue_level_v elvl ON fa.account_id = elvl.first_level
LEFT JOIN adempiere.c_bankstatement bs ON fa.record_id = bs.c_bankstatement_id AND fa.ad_table_id = 392
LEFT JOIN adempiere.c_bpartner bp ON fa.c_bpartner_id = bp.c_bpartner_id

WHERE fa.ad_client_id =    $P{AD_Client_ID} AND fa.ad_org_id =   $P{AD_Org_ID} 
		AND elvl.second_level IN (1000233, 1000259)  AND bs.docstatus IN ('CO', 'CL')
		AND fa.dateacct BETWEEN   $P{StartDate}  AND $P{EndDate}   AND bp.c_bp_group_id IN (1000004, 1000005, 1000006)
		
		
UNION ALL

SELECT 0 opening_cash, 0 opening_bank, 0 cash_receipts_dis, 0 cash_receipts_oth,
		0 bank_receipts_dis, 0 bank_receipts_oth, 0 vend_payments_cash, 0 other_payments_cash, 0 vend_payments_bank,  sum(fa.amtacctcr) other_payments_bank

FROM adempiere.fact_acct fa
LEFT JOIN adempiere.c_elementvalue elv ON fa.account_id = elv.c_elementvalue_id
LEFT JOIN adempiere.c_elementvalue_level_v elvl ON fa.account_id = elvl.first_level
LEFT JOIN adempiere.c_bankstatement bs ON fa.record_id = bs.c_bankstatement_id AND fa.ad_table_id = 392
LEFT JOIN adempiere.c_bpartner bp ON fa.c_bpartner_id = bp.c_bpartner_id

WHERE fa.ad_client_id =    $P{AD_Client_ID} AND fa.ad_org_id =   $P{AD_Org_ID} 
		AND elvl.second_level IN (1000233, 1000259)  AND bs.docstatus IN ('CO', 'CL')
		AND fa.dateacct BETWEEN   $P{StartDate}  AND $P{EndDate}   AND bp.c_bp_group_id NOT IN (1000004, 1000005, 1000006)
 
 ) d]]>
