	<queryString language="SQL">
		<![CDATA[SELECT clientname, orgname, departmet
, MAX(period) period
, MAX(period_start::date) as period_start
, MAX(period_end::date) as period_end
, (MAX(period_end::date) - MAX(period_start::date))+1 as perioddays
, COUNT(empcode) empcode, SUM(gross_salary) gross_salary, SUM(p_gross) p_gross
, SUM(p_basic) p_basic, SUM(p_hr) p_hr, SUM(p_conv) p_conv
, SUM(p_mobile) p_mobile, SUM(p_dalocal) d_dalocal, SUM(p_daout) p_daout
, SUM(p_talocal) p_talocal, SUM(p_taout) p_taout, SUM(p_fuel) p_fuel, SUM(p_vmain) p_vmain, SUM(p_vrent) p_vrent
, SUM(p_nsallow) p_nsallow, SUM(p_nstay) p_nstay, SUM(p_stamp) p_stamp, SUM(p_misc) p_misc, SUM(p_advsal) p_advsal, SUM(p_loan) p_loan, SUM(p_exdays) p_exdays, SUM(p_late) p_late, SUM(p_ot) p_ot, SUM(p_arrear) p_arrear
, SUM(p_comm) p_comm, SUM(p_tax) p_tax , SUM(p_vmain) + SUM(p_vrent) p_vtotal, SUM(p_bankpay) p_bankpay
, (SUM(p_gross) + SUM(p_mobile) + SUM(p_dalocal) + SUM(p_daout) + SUM(p_talocal) + SUM(p_taout) + SUM(p_fuel) + SUM(p_vmain) + SUM(p_vrent) + SUM(p_comm) + SUM(p_nsallow) + SUM(p_nstay) + SUM(p_stamp) + SUM(p_misc) + SUM(p_exdays)+ SUM(p_ot) + SUM(p_arrear)) 
- (SUM(p_tax) + SUM(p_advsal) + SUM(p_loan) + SUM(p_late)  + SUM(p_bankpay) ) p_netsal 
FROM (
SELECT emp.*
, to_char(hp.enddate::date, 'fmMon-yyyy') as period
, hp.startdate::date as period_start
, hp.enddate::date as period_end
, (hp.enddate::date - hp.startdate::date)+1 as perioddays
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000003 AND hr_period_id = hp.hr_period_id) as p_gross
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000032 AND hr_period_id = hp.hr_period_id) as p_basic
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000033 AND hr_period_id = hp.hr_period_id) as p_hr
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000034 AND hr_period_id = hp.hr_period_id) as p_conv
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000035 AND hr_period_id = hp.hr_period_id) as p_wd
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000036 AND hr_period_id = hp.hr_period_id) as p_mobile
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000037 AND hr_period_id = hp.hr_period_id) as p_dalocal
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000038 AND hr_period_id = hp.hr_period_id) as p_daout
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000039 AND hr_period_id = hp.hr_period_id) as p_talocal
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000040 AND hr_period_id = hp.hr_period_id) as p_taout
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000041 AND hr_period_id = hp.hr_period_id) as p_fuel
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000042 AND hr_period_id = hp.hr_period_id) as p_vmain
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000043 AND hr_period_id = hp.hr_period_id) as p_vrent
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000044 AND hr_period_id = hp.hr_period_id) as p_comm
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000045 AND hr_period_id = hp.hr_period_id) as p_tax
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000046 AND hr_period_id = hp.hr_period_id) as p_nsallow
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000047 AND hr_period_id = hp.hr_period_id) as p_nstay
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000048 AND hr_period_id = hp.hr_period_id) as p_stamp
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000049 AND hr_period_id = hp.hr_period_id) as p_misc
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000050 AND hr_period_id = hp.hr_period_id) as p_advsal
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000051 AND hr_period_id = hp.hr_period_id) as p_loan
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000052 AND hr_period_id = hp.hr_period_id) as p_exdays
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000053 AND hr_period_id = hp.hr_period_id) as p_late
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000054 AND hr_period_id = hp.hr_period_id) as p_ot
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000055 AND hr_period_id = hp.hr_period_id) as p_arrear
, (SELECT COALESCE(amount,0) FROM adempiere.hr_movement WHERE c_bpartner_id = emp.c_bpartner_id AND hr_concept_id = 1000056 AND hr_period_id = hp.hr_period_id) as p_bankpay
FROM adempiere.xxhr_employee_v emp
   , adempiere.hr_period hp
WHERE emp.startdate <= hp.enddate::date
  AND hp.hr_period_id = $P{HR_Period_ID}
  AND emp.ad_client_id = $P{AD_Client_ID}
  AND emp.ad_org_id = COALESCE($P{AD_Org_ID}, emp.ad_org_id)
  AND emp.HR_emplocation_ID = COALESCE($P{HR_emplocation_ID}, emp.HR_emplocation_ID)
  AND emp.c_city_ID = COALESCE($P{C_City_ID}, emp.c_city_ID)
  AND (emp.enddate IS NULL OR emp.enddate::date >= hp.startdate::date)
  ) q
  GROUP BY clientname, orgname, departmet
  ORDER BY clientname, orgname, departmet]]>
	</queryString>
