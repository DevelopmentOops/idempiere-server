	<queryString>
		<![CDATA[SELECT Left(account.value,1) l1code,level1.name l1Name,Left(account.value,5) l2code,level2.name l2Name,Left(account.value,9) l3code,level3.name l3Name,
		account.c_elementvalue_id, account.value, account.name,
		CASE WHEN balance > 0  THEN ABS(balance)  ELSE 0 
		END balancecr_dr,
		CASE WHEN balance < 0  THEN ABS(balance) ELSE 0 END balancecr_cr,
		(SELECT org.orgname FROM (SELECT org.name orgname FROM adempiere.ad_org org WHERE CAST(org.ad_org_id as integer) = CAST($P{AD_Org_ID} as integer))org),
	(SELECT cl.clientname FROM (SELECT cl.name clientname FROM adempiere.ad_client cl WHERE CAST(cl.ad_client_id as integer) = CAST($P{AD_Client_ID} as integer))cl)

FROM

(SELECT fa.account_id,(sum(fa.amtacctdr) - sum(fa.amtacctcr)) balance

FROM adempiere.fact_acct fa

WHERE	fa.ad_client_id =  $P{AD_Client_ID} AND ($P!{AD_Org_ID} IS NULL OR fa.ad_org_id = $P!{AD_Org_ID}) 
				AND dateacct <= $P{AsOn}


GROUP BY fa.account_id)amt

RIGHT JOIN

(SELECT elv.c_elementvalue_id, elv.value, elv.name, elv.accounttype

FROM adempiere.c_elementvalue elv

WHERE elv.ad_client_id = $P{AD_Client_ID}

ORDER BY elv.value) account

ON amt.account_id = account.c_elementvalue_id
Join adempiere.c_elementvalue level1 on LEFT(account.value,1) = level1.value  and level1.ad_client_id =  $P{AD_Client_ID}
Join adempiere.c_elementvalue level2 on LEFT(account.value,5) = level2.value  and level2.ad_client_id =  $P{AD_Client_ID}
Join adempiere.c_elementvalue level3 on LEFT(account.value,9) = level3.value  and level3.ad_client_id =  $P{AD_Client_ID}

WHERE  balance !=0
ORDER BY l1code, l2code,l3Code, account.value]]>
